diff --git a/src/main/java/net/minecraft/server/level/ServerLevel.java b/src/main/java/net/minecraft/server/level/ServerLevel.java
index ed5e6232c7fce5ff309009f937f65cba7574eedb..1f193ca8bc0821bb6d01ae6c3914e3e3fb128a60 100644
--- a/src/main/java/net/minecraft/server/level/ServerLevel.java
+++ b/src/main/java/net/minecraft/server/level/ServerLevel.java
@@ -221,6 +221,13 @@ public class ServerLevel extends Level implements WorldGenLevel {
         return new Throwable(entity + " Added to world at " + new java.util.Date());
     }
 
+    // Mirai start
+    @Override
+    public ProfilerFiller getProfiler() {
+        return this.getServer().getProfiler();
+    }
+    // Mirai end
+
     @Override public LevelChunk getChunkIfLoaded(int x, int z) { // Paper - this was added in world too but keeping here for NMS ABI
         return this.chunkSource.getChunkAtIfLoadedImmediately(x, z); // Paper
     }
